
 SYSTEM=sub_system_murrell.f
test=f

here=`pwd`
DIR_vib=$here/../..
DIR_pot=$DIR_vib/sub_pot

 cp $SYSTEM $DIR_pot/sub_system.f

cd $DIR_vib
 make OPT=0
cd $here

#export OMP_NUM_THREADS=1

 x1=0.538516066
 x2=0.461483933


